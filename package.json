{
  "name": "searchngo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -sourcemap -p .",
    "start": "node -r dotenv/config dist/app.js dotenv_config_path=dist/src/env/.env",
    "predev": "npm run lint:fix",
    "dev": "nodemon --watch src -r dotenv/config app.ts dotenv_config_path=src/env/.env",
    "lint": "yarn prettier './src/**/*.ts' && eslint './src/**/*.ts'",
    "lint:fix": "yarn prettier --write ./src/**/*.ts"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express-status-monitor": "^1.3.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.2.1",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@joi/date": "^2.1.0",
    "@types/cron": "^2.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "cron": "^2.3.1",
    "dayjs": "^1.11.11",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.1.1",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2"
  },
  "engines": {
    "npm": "Please use yarn instead of npm",
    "yarn": ">= 1.22.18",
    "node": ">=16.0.0"
  }
}
